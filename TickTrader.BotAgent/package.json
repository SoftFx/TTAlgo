{
  "name": "AlgoServer-WebSpa",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "clean_all": "npm run clean_nodes && npm run clean_dist",
    "clean_nodes": "rimraf node_modules",
    "clean_dist": "rimraf WebAdmin/wwwroot/dist",
    "fix_angular2_polyfill": "node ./fix-angular2-polyfill.js",
    "fix_angular2_polyfill_old": "xcopy node_modules\\reflect-metadata node_modules\\angular2-universal-polyfills\\node_modules\\reflect-metadata /E/H/C/I/Y",
    "build_dev": "npm run fix_angular2_polyfill && npm run build_dev_vendor && npm run build_dev_main",
    "build_dev_vendor": "webpack --mode development --config WebAdmin/webpack.config.vendor.js",
    "build_dev_main": "webpack --mode development --config WebAdmin/webpack.config.js",
    "build_prod": "npm run fix_angular2_polyfill && npm run build_prod_vendor && npm run build_prod_main",
    "build_prod_vendor": "webpack --mode production --color --config WebAdmin/webpack.config.vendor.js",
    "build_prod_main": "webpack --mode production --color --config WebAdmin/webpack.config.js"
  },
  "dependencies": {
    "@angular/common": "~2.2.2",
    "@angular/compiler": "~2.2.2",
    "@angular/core": "~2.2.2",
    "@angular/forms": "~2.2.2",
    "@angular/http": "~2.2.2",
    "@angular/platform-browser": "~2.2.2",
    "@angular/platform-browser-dynamic": "~2.2.2",
    "@angular/router": "~3.2.2",
    "@angular/upgrade": "~2.2.2",
    "@microsoft/signalr": "6.0.5",
    "angular-in-memory-web-api": "^0.2.0",
    "angular2-jwt": "0.1.28",
    "angular2-masonry": "0.4.0",
    "bootstrap": "^3.3.7",
    "bootstrap-notify": "3.1.3",
    "es6-promise": "^4.2.8",
    "es6-shim": "^0.35.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "font-awesome": "4.7.0",
    "jquery": "^3.6.0",
    "masonry-layout": "4.1.1",
    "promise-polyfill": "8.2.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.6",
    "rxjs-compat": "^6.6.6",
    "string-format": "0.5.0",
    "tslib": "^1.10.0",
    "uglify-js-es6": "^2.8.9",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "es6-promise": "4.2.8",
    "@angular/platform-server": "2.0.0",
    "@types/core-js": "^0.9.34",
    "@types/jquery": "3.3.31",
    "@types/node": "^6.0.62",
    "@types/signalr": "^2.2.35",
    "angular2-platform-node": "^2.1.0-rc.1",
    "angular2-universal": "^2.1.0-rc.1",
    "angular2-universal-polyfills": "^2.1.0-rc.1",
    "copy-webpack-plugin": "^8.0.0",
    "css-loader": "^0.25.0",
    "expose-loader": "^2.0.0",
    "mini-css-extract-plugin": "^1.3.9",
    "style-loader": "^0.13.0",
    "timers-browserify": "^2.0.12",
    "to-string-loader": "^1.1.5",
    "ts-loader": "8.0.18",
    "typescript": "3.6.3",
    "webpack": "^5.26.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-hot-middleware": "2.17.1"
  }
}
