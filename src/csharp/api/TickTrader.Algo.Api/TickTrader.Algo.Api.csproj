<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>algo.snk</AssemblyOriginatorKeyFile>
    <VersionPrefix>1.5.3</VersionPrefix>
    <FileVersion>$(VersionPrefix).0</FileVersion>
  </PropertyGroup>

  <PropertyGroup Label="NuGet package definition">
    <Id>TickTrader.Algo.Api</Id>
    <Title>TickTrader.Algo.Api</Title>
    <Description>TickTrader Algo Api for bots and indicators</Description>
    <Tags>SoftFX;TickTrader</Tags>
    <Authors>Soft-FX</Authors>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseExpression>MPL-2.0</PackageLicenseExpression>
    <PackageIcon>images\icon.png</PackageIcon>
    <RepositoryUrl>https://github.com/SoftFx/TTAlgo</RepositoryUrl>
    <PackageProjectUrl>https://github.com/SoftFx/TTAlgo</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <None Include="icon.png" Pack="true" PackagePath="images" />
  </ItemGroup>

  <ItemGroup>
    <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
      <_Parameter1>TickTrader.Algo.CoreV1,</_Parameter1>
    </AssemblyAttribute>
    <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
      <_Parameter1>TickTrader.Algo.Backtester,</_Parameter1>
    </AssemblyAttribute>
  </ItemGroup>

</Project>
