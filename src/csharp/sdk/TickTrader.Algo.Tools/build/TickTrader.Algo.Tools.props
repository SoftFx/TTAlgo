    <AlgoPackage_OutputPath Condition="'$(AlgoPackage_OutputPath)' == ''"></AlgoPackage_OutputPath>
    <AlgoPackage_BuildMetadata Condition="'$(AlgoPackage_BuildMetadata)' == ''">false</AlgoPackage_BuildMetadata>
