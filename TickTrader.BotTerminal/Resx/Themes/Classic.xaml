<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:l="clr-namespace:TickTrader.BotTerminal">

    <SolidColorBrush x:Key="Shell_PrimaryForeground" Color="Black"/>
    <SolidColorBrush x:Key="Shell_SecondaryForeground" Color="Gray"/>
    <SolidColorBrush x:Key="Shell_MediateForeground" Color="Gray"/>
    <SolidColorBrush x:Key="Shell_WarningForeground" Color="#DA5A3A"/>
    <SolidColorBrush x:Key="Shell_ErrorForeground" Color="#9F4747"/>
    <SolidColorBrush x:Key="Shell_SuccessForeground" Color="DarkGreen"/>

    <SolidColorBrush x:Key="Shell_PrimaryBackground" Color="White"/>
    <SolidColorBrush x:Key="Shell_SecondaryBackground" Color="LightGray"/>

    <SolidColorBrush x:Key="DataGrid_SelectionBackground" Color="LightBlue" />

    <SolidColorBrush x:Key="RateUpColor" Color="#009344"/>
    <SolidColorBrush x:Key="RateUpHighlightColor" Color="#006836"/>
    <SolidColorBrush x:Key="RateDownColor" Color="#F15923"/>
    <SolidColorBrush x:Key="RateDownHighlightColor" Color="#F03E00"/>
    <SolidColorBrush x:Key="RateUnknownColor" Color="#808080"/>
    <SolidColorBrush x:Key="RateUnknownHighlightColor" Color="#6E6E6E"/>

    <SolidColorBrush x:Key="Dialog_Background" Color="White"/>
    <SolidColorBrush x:Key="Dialog_ErrorBackground" Color="#FCB1B2"/>
    <SolidColorBrush x:Key="Dialog_ErrorForeground" Color="Red"/>
    <SolidColorBrush x:Key="Dialog_ControlDisabledBackground" Color="LightGray"/>
    <SolidColorBrush x:Key="Dialog_OkButtonBackground" Color="White"/>
    <SolidColorBrush x:Key="Dialog_OkButtonHighlightBackground" Color="LightGray"/>
    <SolidColorBrush x:Key="Dialog_ButtonBackground" Color="White"/>
    <SolidColorBrush x:Key="Dialog_ButtonHighlightBackground" Color="LightGray"/>
    <SolidColorBrush x:Key="Dialog_ButtonBorderColor" Color="Gray"/>

    <SolidColorBrush x:Key="Chart_BotControl_Background" Color="#3B3B3B" />
    <SolidColorBrush x:Key="Chart_BotControl_HighlightBackground" Color="#292929" />
    <SolidColorBrush x:Key="Chart_BotControl_StartBackground" Color="#29ABE2" />
    <SolidColorBrush x:Key="Chart_BotControl_StopBackground" Color="Red" />

    <SolidColorBrush x:Key="SellColor" Color="#009344"/>
    <SolidColorBrush x:Key="BuyColor" Color="#F15923"/>

    <SolidColorBrush x:Key="ChartBackground" Color="White" />
    <SolidColorBrush x:Key="ChartGridLinesColor" Color="LightGray" />

    <SolidColorBrush x:Key="ToolBarItem_ForegroundBrush" Color="#808080"/>
    <SolidColorBrush x:Key="ToolBarItem_HighlightForegroundBrush" Color="#000000"/>

    <SolidColorBrush x:Key="SoftFxLogo_BackgroundBrush" Color="#FFCFD641"/>

    <ResourceDictionary.MergedDictionaries>
        <l:AutoDictionary Source="Resx/Styles.Classic/*.xaml"/>
    </ResourceDictionary.MergedDictionaries>

</ResourceDictionary>